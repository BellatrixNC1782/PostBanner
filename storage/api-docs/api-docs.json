                    "description": "Pass user credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "is_social",
                                    "login_with",
                                    "social_id",
                                    "device_token",
                                    "device_type",
                                    "uu_id"
                                ],
                                "properties": {
                                    "social_id": {
                                        "type": "string",
                                        "example": "google_id/apple_id"
                                    },
                                    "login_with": {
                                        "type": "string",
                                        "example": "Email/Google/Apple"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "cavin@mailinator.com"
                                    },
                                    "device_token": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "123"
                                    },
                                    "device_type": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Android"
                                    },
                                    "device_model": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Samsung SM-M146B - 13"
                                    },
                                    "device_os": {
                                        "type": "integer",
                                        "example": "os"
                                    },
                                    "app_version": {
                                        "type": "integer",
                                        "example": "version"
                                    },
                                    "api_version": {
                                        "type": "integer",
                                        "example": "api version"
                                    },
                                    "uu_id": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "123"
                                    },
                                    "apple_auth_code": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "123avcaca123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "422": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong email address. Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
